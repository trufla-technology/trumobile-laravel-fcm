{
    "name": "trufla-technology/trumobile-laravel-fcm",
    "description": "Laravel / Lumen package for Firebase Cloud Messaging ",
    "keywords": [
        "laravel",
        "lumen",
        "firebase",
        "notification",
        "push",
        "fcm",
        "firebase cloud messaging"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Trufla Technology",
            "email": "info@trufla.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9",
        "guzzlehttp/guzzle": "^7.5",
        "monolog/monolog": "^2.8"
    },
    "require-dev": {
        "laravel/laravel": "9.*",
        "mockery/mockery": "^1.5",
        "satooshi/php-coveralls": "*",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "TruMobileLaravelFCM\\": "src/",
            "TruMobileLaravelFCM\\Mocks\\": "tests/mocks"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "TruMobileLaravelFCM\\FCMServiceProvider"
            ],
            "aliases": {
                "FCM": "TruMobileLaravelFCM\\Facades\\FCM",
                "FCMGroup": "TruMobileLaravelFCM\\Facades\\FCMGroup"
            }
        }
    }
}
